
# ตัวอย่างสคริปต์ AI Language
start create ML

# โหลดข้อมูล
load dataset "datasets/linear_data.csv" type "csv"

# ทำความสะอาดข้อมูล
clean data with drop_na true

# แบ่งข้อมูล
split data into train, test with ratio 0.8, 0.2

# สร้างโมเดล
create model LinearRegression

# ตั้งค่าพารามิเตอร์ (ไม่จำเป็นต้องมี, จะใช้ค่าอัตโนมัติ)
set learning_rate 0.01
set epochs 100

# ฝึกโมเดล
train model

# ประเมินโมเดล
evaluate model

# แสดงเมทริก
show metric "accuracy"
show metric "r2_score"

# บันทึกโมเดล
save model to "model.pkl"
